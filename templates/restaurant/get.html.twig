			    <table class="table">
			        {% for number in number %}
					    <tr>
						   <td>lundi:</td>
						   <td>{{number.lundi}}</td>
					    </tr>
					    <tr>
						   <td>mardi:</td>
						   <td>{{number.mardi}}</td>
					    </tr>
						<tr>
						   <td>mercredi:</td>
