					    </tr>
						<tr>
						   <td>jeudi:</td>
						   <td>{{number.jeudi}}</td>
					    </tr>
					    <tr>
						   <td>vendredi:</td>
						   <td>{{number.vendredi}}</td>
					    </tr>
					    <tr>
						   <td>samedi:</td>
						   <td>{{number.samedi}}</td>
					    </tr>
						<tr>
						   <td>dimanche:</td>
						   <td>{{number.dimanche}}</td>
					    </tr>
				    {% endfor %}
				  
			    </table>
  
    </div></div></div>
{% endblock %}